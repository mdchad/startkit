<div class="col-md-4 col-md-offset-4">
  <h1><%= data.fullName %></h1>
  <p><%= data.email %></p>
  <p><%= data.job %></p>
  <a href="/profile/edit/<%= data.id %>" class="button tiny">Edit</a>
  <hr>
  <% for(var i = 0; i < list.length ; i++ ){ %>
    <div class="row">
      <div class="col-sm-6 col-md-12 "><a class='card1' href="idea/<%= list[i].id %>">
        <div class="thumbnail">
          <div class="caption">
            <h3 class="light"><%= list[i].title %></h3>
            <h5 class="light"><%= list[i].industry %></h5>
          </div>
        </div></a>
      </div>
    </div>
  <% } %>
</div>
